<div class="tuimg more">
    <div class="tu-rightimg tu-more">

        <ul id="parentUl" >
            <% rawData.forEach(function(item){ %>
            <li id="parentli" class="sign">
                <!-- 触发弹窗 -->
                <img loading="lazy" id="myImg" class="myImg"  src="<%= item.ImgUrl %>" alt="《<%= item.worksName %>》-<%= item.Author %>" width="300" height="200" />
                <p>《<%= item.worksName %>》-<%= item.Author %></p>
            </li>
            <% }) %>
                <!-- 弹窗  开始-->
                <div id="myModal" class="modal" >
                    <!-- 关闭按钮 -->
                    <span class="close"  onclick="document.getElementById('myModal').style.display='none'">&times;</span>
                    <!-- 弹窗内容 -->
                    <img class="modal-content"   id="img01"  />
                    <!-- 文本描述 -->
                    <div id="caption"></div>
                </div>
                <!-- 弹窗  结束-->
        </ul>


    </div>
