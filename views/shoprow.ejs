</head>
<body>
<!-- 导航栏 -->
<div class="header">
    <ul>
        <li class="">
            <a href="/index">首页</a>
        </li>
        <li class="active">
            <a href="collect">电子收藏</a>
        </li>
        <!-- 搜索栏 -->
        <!--        <li class="search">-->
        <!--            <input class="input" type="text" placeholder="请输入您要搜索的商品" />-->
        <!--            <input class="input2" type="submit" />-->
        <!--        </li>-->
    </ul>
</div>
<div >
    <div  class="container">
        <form action="/shoprow" name="form2" method="post"  >
            <div>
<!--        <div style="position:absolute;width: 80%;height: 100vh;background-color: rgba(217,83,79,0.36);z-index: -1;">adhsf</div>-->
                <!-- 商品图片-->
                <input name="goods" class="goods" id="Img" type="image"  src=" " width="400px"  disabled="disabled"  />

            </div>
            <div class="describe">
                <!-- 商品标题-->
                <input name="goodsName" class="goodName"  id="captitle" type="text" readonly="readonly" />

                <!-- 商品描述-->
                <textarea name="explain" class="explain" id="captext" type="text"  readonly="readonly" ></textarea>

                <!--   商品价格-->
                <input name="price" class="price" id="capmon" type="text" readonly="readonly" />
                <div name="quantity">
                    <div class="plussub" id="plussubs" >
                        <span id="subs" class="subs"  >&ndash;</span>
                        <input name="quantity" class="quantity" id="quantitys" type="text" value="1" readonly="readonly">
                        <span id="pluss" class="pluss" >&#43;</span>
                    </div>
                </div>

            <div  class="submit">
                <button id="car"  class="cancel" >加入收藏</button>
                <span  id="button"  class="determine">立即购买</span>
            </div>
            </div>
        </form>
    </div>
</div>
<!--底部声明-->
