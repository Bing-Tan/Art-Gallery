</div>
<!--加入购物车弹窗--开始-->
<div id="isDialog">
    <div class="dialog isDialog"></div>
    <div class="dialog-wrap isDialog"  >

        <form  action="/buy" method="post">
            <!-- 关闭按钮 -->
            <span class="close" style="font-size: 30px;float:right;opacity: 0.6 "   onclick="document.getElementById('isDialog').style.display='none'">&times;</span>
            <!-- 商品标题-->
            <input name="goodsName" class="dialog-header" id="goodsName" readonly="readonly" />
            <!-- 图片与描述-->
            <div>
                <!-- 商品图片-->
                <input style="margin-left: 15%;" name="goodsUrl" type="image" id="img01"  width="400px" disabled="disabled" />

                <!-- 商品描述-->
                <input class="explains" id="explain"  style="font-size: 15px;margin-top:20px;text-align: center;color: #707070;" name="explain" readonly="readonly"   />
            </div>
            <div >
                <!--   商品价格-->
                <input class="prices"  id="price" name="price"  readonly="readonly" />
            </div>
            <div name="quantity">
                <div class="plussubs" id="plussub" >
                    请输入数量：
                    <input name="quantity" class="quantitys" id="quantity" type="text"  value="1" >
                </div>
            </div>
            <div class="dialog-footer">
                <span class="cancel" onclick="document.getElementById('isDialog').style.display='none'">取消</span>
                <button  class="determine" >确认购买</button>
            </div>
        </form>
    </div>
</div>
<!--加入购物车弹窗--结束-->
</body>

